{
  "name": "jest-typescript-boilerplate",
  "version": "1.0.0",
  "repository": "https://github.com/memoriaXII/jest-typescript-boilerplate.git",
  "author": "memoriaXII",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next -p 8080",
    "build": "next build",
    "start": "next start",
    "export": "yarn-or-npm build && next export",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test:coverage": "yarn-or-npm test --coverage",
    "test:debug": "yarn-or-npm test --debug --detectOpenHandles",
    "test:ci": "yarn-or-npm test --runInBand --no-cache --ci",
    "test": "NODE_ENV=test jest",
    "test:e2e": "npx playwright test e2e",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf out/",
    "prepare": "husky install",
    "build-storybook": "build-storybook -s ./public/static  -c .storybook -o .storybookout"
  },
